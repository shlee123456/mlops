{
  "dashboard": {
    "title": "Inference Metrics",
    "tags": ["mlops", "inference", "serving", "fastapi"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "uid": "inference-metrics",
    "editable": true,
    "annotations": {
      "list": []
    },
    "panels": [
      {
        "id": 1,
        "title": "üìä HTTP Request Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[1m])) by (method, endpoint)",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0,
            "color": {"mode": "palette-classic"}
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"}
        }
      },
      {
        "id": 2,
        "title": "‚è±Ô∏è HTTP Request Latency (p50, p95, p99)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "p50 - {{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "p95 - {{endpoint}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "p99 - {{endpoint}}",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 3,
        "title": "üî¢ Active Requests",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum(http_requests_in_progress)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value"
        }
      },
      {
        "id": 4,
        "title": "‚úÖ Success Rate",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status_code=~\"2..\"}[5m])) / sum(rate(http_requests_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "üìà Total Requests",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "sum(http_requests_total)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value"
        }
      },
      {
        "id": 6,
        "title": "‚ùå Error Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status_code=~\"5..\"}[5m])) / sum(rate(http_requests_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 7,
        "title": "ü§ñ LLM Request Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "sum(rate(llm_requests_total[1m])) by (model, status)",
            "legendFormat": "{{model}} - {{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 8,
        "title": "‚ö° LLM Request Duration",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(llm_request_duration_seconds_bucket[5m])) by (le, model))",
            "legendFormat": "p50 - {{model}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(llm_request_duration_seconds_bucket[5m])) by (le, model))",
            "legendFormat": "p95 - {{model}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0
          }
        }
      },
      {
        "id": 9,
        "title": "ü™ô Tokens Generated Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "sum(rate(llm_tokens_generated_total[1m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "displayName": "Tokens/sec"
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 10,
        "title": "üíæ Total Tokens Generated",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 20},
        "targets": [
          {
            "expr": "sum(llm_tokens_generated_total)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 11,
        "title": "üóÑÔ∏è Active DB Connections",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 20},
        "targets": [
          {
            "expr": "db_connections_active",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 12,
        "title": "üìù FastAPI Logs",
        "type": "logs",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 24},
        "datasource": {"type": "loki", "uid": "loki"},
        "targets": [
          {
            "expr": "{job=\"fastapi\"} | json",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "wrapLogMessage": true,
          "sortOrder": "Descending",
          "enableLogDetails": true,
          "dedupStrategy": "none"
        }
      },
      {
        "id": 13,
        "title": "üìä Request by Status Code",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 34},
        "targets": [
          {
            "expr": "sum(http_requests_total) by (status_code)",
            "legendFormat": "{{status_code}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right"},
          "pieType": "pie"
        }
      },
      {
        "id": 14,
        "title": "üõ§Ô∏è Request by Endpoint",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 34},
        "targets": [
          {
            "expr": "sum(http_requests_total) by (endpoint)",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right"},
          "pieType": "pie"
        }
      },
      {
        "id": 15,
        "title": "‚ÑπÔ∏è Application Info",
        "type": "table",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 34},
        "targets": [
          {
            "expr": "fastapi_app_info",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "Value": true, "__name__": true}
            }
          }
        ]
      }
    ]
  }
}
